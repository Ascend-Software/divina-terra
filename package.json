{
  "name": "divina-terra",
  "description": "A landing page for the best market specializing in healthy food",
  "homepage": "https://www.divinaterraatibaia.com.br",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "check": "astro check",
    "preview": "astro preview",
    "lint": "eslint --ext .js,.ts,.tsx,.astro .",
    "format": "prettier --write src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/tailwind": "^5.0.2",
    "@builder.io/qwik": "^1.2.19",
    "@qwikdev/astro": "^0.2.2",
    "astro": "^3.6.0",
    "astro-seo": "^0.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/cz-commitlint": "^18.4.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.54.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-qwik": "^1.2.19",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "tailwindcss": "^3.3.5",
    "tsc-files": "^1.1.4",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,astro}": [
      "tsc-files --noEmit",
      "eslint --fix",
      "prettier --write --plugin=prettier-plugin-astro"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
